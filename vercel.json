{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/health",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "CONNECTION_STRING": "@connection_string",
    "ACCESS_TOKEN_SECRET": "@access_token_secret",
    "NODE_ENV": "production"
  }
}